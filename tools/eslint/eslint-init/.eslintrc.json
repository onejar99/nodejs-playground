{
    "extends": "airbnb-base",
    "parserOptions":{
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "browser": false,
        "es6": true
    },
    "rules": {
        "comma-dangle":"warn",
        "indent": ["warn", 4],
        "no-unused-vars":[
            "error",
            {
                "vars": "local",
                "args": "none"
            }
        ]
    }
}
